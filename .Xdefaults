! xterm                                                                          !
!                                                                                !
!---------------------------------------------------------------------------------

xterm*utf8:			1

xterm*faceName:   	Ubuntu Mono for Powerline
xterm*faceSize:   	11
xterm*termName:   	xterm-256color
xterm*font: 		fixed
xterm*boldFont: 	fixed

xterm*transparent: 	true
xterm*shading:		150
!xterm*saveLines:	12000
xterm*color1:		SteelBlue
