!!xterm*color9:		black
!!xterm*color10:		black
!!xterm*color11:		black
!!xterm*color12:		steelblue
!!xterm*color13:		black
!! bg color of package numbers in yaourt, matching parentheses in vim
xterm*color14:		darkblue

