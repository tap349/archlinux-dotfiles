!---------------------------------------------------------------------------------
!                                                                                !
! urxvt                                                                          !
!                                                                                !
!---------------------------------------------------------------------------------

!---------------------------------------------------------------------------------
! transparency
!---------------------------------------------------------------------------------
!urxvt*transparent: 	false
urxvt*transparent: 	true

!---------------------------------------------------------------------------------
! background image
!---------------------------------------------------------------------------------
!urxvt*inheritPixmap: 	true
! add ":root" to make pixmap act as wallpaper
!urxvt*backgroundPixmap: /home/ishitori/pictures/aha/temp/hq_bw.xpm;40x40+50+50:root

!---------------------------------------------------------------------------------
! transparency options and bg/fg colors
!---------------------------------------------------------------------------------
! <100 - darkens; >100 - lightens
urxvt*shading:		40
urxvt*tintColor:	ivory
urxvt*saveLines:	32000
!urxvt*blendType:	overlay
!urxvt*blurRadius:	0
!urxvt*foreground:	#002233
urxvt*foreground:	#eeeeee
!urxvt*background:	#c5d5df
!urxvt*background:	#ffffff
!urxvt*background:	#fffff9
!urxvt*background:	#222225
urxvt*background:	#1C252C
urxvt*buffered:		true

!---------------------------------------------------------------------------------
! font
!---------------------------------------------------------------------------------

urxvt*font:             -*-fixed-medium-r-semicondensed-*-13-120-*-*-*-*-iso10646-*
urxvt*boldFont:  		-*-fixed-medium-r-semicondensed-*-13-120-*-*-*-*-iso10646-*

!urxvt*font:			xft:Anonymous Pro for Powerline:style=Regular:pixelsize=16:antialias=true:hinting=true
!urxvt*boldFont:		xft:Anonymous Pro for Powerline:style=Bold,BoldForPowerline:pixelsize=16:antialias=true:hinting=true

!URxvt.letterSpace: -3

!---------------------------------------------------------------------------------
! other options
!---------------------------------------------------------------------------------
urxvt*internalBorder:   0
urxvt*scrollBar:	false
!urxvt*metaSendsEscape:	true
!urxvt*altSendsEscape:	true
!urxvt*eightBitInput:	false

!---------------------------------------------------------------------------------
! colors
!---------------------------------------------------------------------------------
!URxvt*colorBD:          black
! foreground colour
!URxvt*color0:          #444444
!URxvt*color1:          #007FFF
URxvt*color1:          	steelblue
! selection, main font color in vim, top line in htop
!URxvt*color2:           #888888
!URxvt*color2:           #228B22
!!URxvt*color2:           #9B7765
! memory fg color in htop
URxvt*color3:           SteelBlue
! bg color in mcedit and mc, comments in vi and config files
! URxvt*color4:           #27408B
URxvt*color4:           #104363
!URxvt*color5:          black
! selected line bg color and extra numbers in htop, alias names in .bashrc
!URxvt*color6:           #ababab
URxvt*color6:           lightskyblue
!URxvt*color7:          black
! root name in htop, file name fg color in mc when root
!URxvt*color8:          black
!URxvt*color9:          SkyBlue
!URxvt*color10:         black
!URxvt*color11:         black
! selected line bg color
!URxvt*color12:         royalblue
!URxvt*color13:         black
! links (soft as well)
!!URxvt*color14:          darkblue
! fg color of buttons in dialog boxed (e.g. save changes?)
!URxvt*color15:         black

